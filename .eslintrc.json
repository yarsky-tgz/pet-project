{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "commonjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "newline-per-chained-call": 1,
        "no-return-assign": 1,
        "consistent-return": 1,
        "no-console": 1,
        "max-len": 1,
        "import/extensions": 1,
        "import/no-unresolved": 1
    }
}
